{
  "name": "node-url-to-markdown",
  "version": "1.0.0",
  "description": "A Node.js package to convert web page contents to Markdown.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.mjs"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "markdownlint": "^0.37.4",
    "markdownlint-cli2": "^0.17.2",
    "turndown": "^7.0.0"
  },
  "author": "",
  "license": "ISC",
  "markdownlint-cli2": {
    "config": {
      "default": true,
      "line-length": false,
      "no-inline-html": {
        "allowed_elements": [
          "codapi-snippet",
          "kbd",
          "DocCardList",
          "details",
          "summary",
          "br",
          "iframe",
          "sup"
        ]
      },
      "emphasis-style": {
        "style": "underscore"
      },
      "ul-style": {
        "style": "asterisk"
      }
    }
  }
}
